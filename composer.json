{
    "name": "javiereguiluz/easy-admin-demo",
    "license": "MIT",
    "type": "project",
    "description": "A Symfony demo application to showcase the EasyAdmin bundle features.",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "erusev/parsedown": "^1.6",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^4.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "*",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.0",
        "symfony/requirements-checker": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.0",
        "vich/uploader-bundle": "^1.8",
        "ext-pdo_sqlite": "*",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/security-csrf": "^3.3",
        "symfony/templating": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^3.3",
        "symfony/debug-bundle": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^3.3",
        "symfony/web-server-bundle": "^3.3"
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "security-checker security:check": "script",
            "requirements-checker": "script"
        }
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
